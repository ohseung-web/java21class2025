package example;

public class Hello {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Object obj = new Object();
		System.out.println("Hello java World!!");
//		java 주석 단축키 : Ctrl + /
		int i; //변수 지정
		i = 10; //초기값 지정
		int j=20;
		System.out.println(i);
//		한줄 복사 단축키 Ctrl + Alt + ↓
		System.out.println(j);
		
		
	}

}
