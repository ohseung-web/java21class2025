package example;

public class Test01 {

	public static void main(String[] args) {
		int num = 10;
		System.out.println("1씩 증가 후");
		System.out.println(++num);
		System.out.println("============");
		
		
		int x = 10;
		int y =20;
		// 산술 연산자.
		System.out.println("x + y ="+ (x+y));
		System.out.println("x - y ="+ (x-y));
		System.out.println("x * y ="+ (x*y));
		System.out.println("x / y ="+ (x/y));
		System.out.println("x % y ="+ (x%y));
			
	}

}
